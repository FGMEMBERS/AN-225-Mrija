<PropertyList>

<particlesystem>
	<!-- <type>trail</type>-->
	<name>engine_blast_3</name>
	<texture>puff.rgb</texture>
	<emissive>true</emissive>
	<lighting>false</lighting>
	
	  
	<condition>
		  <greater-than>
		    <property>sim/multiplay/generic/float[19]</property>
		    <value>30</value>
		  </greater-than>
		</condition>
	
	<attach>local</attach>
	
	<placer>
		<type>point</type>
		
	</placer>
	
	<shooter>
		<theta-min-deg>0</theta-min-deg>
		<theta-max-deg>0</theta-max-deg>
		<phi-min-deg>0</phi-min-deg>
		<phi-max-deg>0</phi-max-deg>
		<speed-mps>
			  <expression>
			  <product>
				<property>sim/multiplay/generic/float[19]</property>
				<property>sim/multiplay/generic/float[14]</property>
				<value>0.18</value>
			  </product>
			  </expression>
				<spread>1</spread>
			</speed-mps>
		<rotation-speed>
			<x-min-deg-sec>0</x-min-deg-sec>
			<y-min-deg-sec>0</y-min-deg-sec>
			<z-min-deg-sec>0</z-min-deg-sec>
			<x-max-deg-sec>0</x-max-deg-sec>
			<y-max-deg-sec>0</y-max-deg-sec>
			<z-max-deg-sec>0</z-max-deg-sec>
		</rotation-speed>
	</shooter>
	
	<counter>
		<particles-per-sec>
			<value>20</value>
			<spread>0</spread>
		</particles-per-sec>
		
	</counter>
	
	<align>billboard</align>
	
	<particle>
		<start>
        	<color>
			<red>
				<value>0.8</value>
			</red>
			<green>
				<value>0.4</value>
			</green>
			<blue>
				<value>0.1</value>
			</blue>
			<alpha>
				<value>0.35</value>
			</alpha>
		</color>
        		<size>
          			<value>0.35</value>
        		</size>
      		</start>
		<end>
        	<color>
			<red>
				<value>0.47</value>
			</red>
			<green>
				<value>0.53</value>
			</green>
			<blue>
				<value>0.34</value>
			</blue>
			<alpha>
				<value>0.05</value>
			</alpha>
		</color>
        		<size>
          			<value>0.11</value>
        		</size>
      		</end>

		<life-sec>
			<value>0.2</value>
		</life-sec>
		<mass-kg>5.0</mass-kg>
		<radius-m>0.1</radius-m>
	</particle>
	
	<program>
		<fluid>air</fluid>
		<gravity type="bool">false</gravity>
		<wind type="bool">false</wind>
	</program>
	
</particlesystem>

</PropertyList>